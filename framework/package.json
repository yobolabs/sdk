{
  "name": "@jetdevs/framework",
  "version": "0.2.6-dev",
  "description": "Core infrastructure abstractions for Jetdevs Platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./db": {
      "types": "./dist/db/index.d.ts",
      "import": "./dist/db/index.mjs",
      "require": "./dist/db/index.js"
    },
    "./permissions": {
      "types": "./dist/permissions/index.d.ts",
      "import": "./dist/permissions/index.mjs",
      "require": "./dist/permissions/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.mjs",
      "require": "./dist/auth/index.js"
    },
    "./router": {
      "types": "./dist/router/index.d.ts",
      "import": "./dist/router/index.mjs",
      "require": "./dist/router/index.js"
    },
    "./trpc": {
      "types": "./dist/trpc/index.d.ts",
      "import": "./dist/trpc/index.mjs",
      "require": "./dist/trpc/index.js"
    },
    "./audit": {
      "types": "./dist/audit/index.d.ts",
      "import": "./dist/audit/index.mjs",
      "require": "./dist/audit/index.js"
    },
    "./cache": {
      "types": "./dist/cache/index.d.ts",
      "import": "./dist/cache/index.mjs",
      "require": "./dist/cache/index.js"
    },
    "./telemetry": {
      "types": "./dist/telemetry/index.d.ts",
      "import": "./dist/telemetry/index.mjs",
      "require": "./dist/telemetry/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.mjs",
      "require": "./dist/events/index.js"
    },
    "./patterns": {
      "types": "./dist/patterns/index.d.ts",
      "import": "./dist/patterns/index.mjs",
      "require": "./dist/patterns/index.js"
    },
    "./nextjs": {
      "types": "./dist/nextjs/index.d.ts",
      "import": "./dist/nextjs/index.mjs",
      "require": "./dist/nextjs/index.js"
    },
    "./tenant": {
      "types": "./dist/tenant/index.d.ts",
      "import": "./dist/tenant/index.mjs",
      "require": "./dist/tenant/index.js"
    },
    "./tenant/client": {
      "types": "./dist/tenant/client.d.ts",
      "import": "./dist/tenant/client.mjs",
      "require": "./dist/tenant/client.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "jetdevs",
    "framework",
    "database",
    "permissions",
    "auth"
  ],
  "author": "Jetdevs",
  "license": "PRIVATE",
  "peerDependencies": {
    "drizzle-orm": "^0.44.2",
    "@trpc/server": "^11.1.0",
    "@trpc/client": "^11.1.0",
    "@tanstack/react-query": "^5.0.0",
    "next-auth": "^4.24.11",
    "next": "^15.0.0",
    "zod": "^3.22.4"
  },
  "peerDependenciesMeta": {
    "@trpc/client": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@trpc/client": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "@tanstack/react-query": "^5.90.12",
    "next": "^15.1.9",
    "react": "^19.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5",
    "vitest": "^3.2.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
