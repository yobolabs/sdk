{
  "name": "@jetdevs/cloud",
  "version": "1.0.4-dev",
  "description": "AWS S3 and cloud service integrations for Jetdevs Platform",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js",
      "require": "./dist/storage/index.cjs"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js",
      "require": "./dist/config/index.cjs"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js",
      "require": "./dist/lib/index.cjs"
    },
    "./whatsapp": {
      "types": "./dist/whatsapp/index.d.ts",
      "import": "./dist/whatsapp/index.js",
      "require": "./dist/whatsapp/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "yobo",
    "aws",
    "s3",
    "cloud",
    "storage"
  ],
  "author": "Jetdevs",
  "license": "PRIVATE",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@aws-sdk/lib-storage": "^3.940.0",
    "@aws-sdk/s3-request-presigner": "^3.940.0",
    "axios": "^1.7.9",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^20",
    "dotenv": "^17.2.3",
    "tsup": "^8.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^3.2.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
