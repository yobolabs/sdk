{
  "name": "@jetdevs/packages",
  "version": "1.0.3-dev",
  "private": true,
  "description": "Jetdevs SDK Packages",
  "scripts": {
    "build": "pnpm -r build",
    "build:core": "pnpm --filter @jetdevs/core build",
    "build:framework": "pnpm --filter @jetdevs/framework build",
    "build:cloud": "pnpm --filter @jetdevs/cloud build",
    "publish:packages": "pnpm -r publish --access public --no-git-checks",
    "publish:core": "pnpm --filter @jetdevs/core publish --access public --no-git-checks",
    "publish:framework": "pnpm --filter @jetdevs/framework publish --access public --no-git-checks",
    "publish:cloud": "pnpm --filter @jetdevs/cloud publish --access public --no-git-checks",
    "version:patch": "pnpm -r exec -- npm version patch",
    "version:minor": "pnpm -r exec -- npm version minor"
  }
}
