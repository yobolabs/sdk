{
  "name": "yobo-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Yobo Platform Monorepo - SDKs and Microservices",
  "engines": {
    "node": ">=22.16.0",
    "pnpm": ">=8.6.0"
  },
  "packageManager": "pnpm@8.6.12",
  "scripts": {
    "dev": "pnpm --filter merchant-portal dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "publish:packages": "pnpm build && pnpm -r publish",
    "publish:dry-run": "pnpm build && pnpm -r publish --dry-run"
  },
  "keywords": ["monorepo", "sdk", "microservices"],
  "author": "Yobo",
  "license": "PRIVATE"
}
