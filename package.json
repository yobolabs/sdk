{
  "name": "@yobolabs/packages",
  "version": "1.0.4-dev",
  "private": true,
  "description": "YoboLabs SDK Packages",
  "scripts": {
    "build": "pnpm -r build",
    "build:core": "pnpm --filter @yobolabs/core build",
    "build:framework": "pnpm --filter @yobolabs/framework build",
    "build:cloud": "pnpm --filter @yobolabs/cloud build",
    "publish:packages": "pnpm -r publish --access public --no-git-checks",
    "publish:core": "pnpm --filter @yobolabs/core publish --access public --no-git-checks",
    "publish:framework": "pnpm --filter @yobolabs/framework publish --access public --no-git-checks",
    "publish:cloud": "pnpm --filter @yobolabs/cloud publish --access public --no-git-checks",
    "version:patch": "pnpm -r exec -- npm version patch",
    "version:minor": "pnpm -r exec -- npm version minor"
  }
}
